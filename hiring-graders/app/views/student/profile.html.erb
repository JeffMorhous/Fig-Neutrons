<h2><%=@studentName%>'s Profile</h2>
<a href="/student/application"  class="btn btn-primary">Create & Submit New Application</a>
<table>
  <tr>
    <th>Course Number</th>
    <th>Grade</th>
  </tr>
  <%  @studentGrades.each do |transcript| %>
  <tr>
    <td><%= transcript.grade %></td>
    <td><%= transcript.course_id %></td>
  </tr>
  <% end %>
</table>

