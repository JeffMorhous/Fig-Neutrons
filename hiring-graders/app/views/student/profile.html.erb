<h2><%=@studentName%>'s Profile</h2>
<a href="/student/application"  class="btn btn-primary">Create & Submit Your Grader Application</a>
<table>
  <tr>
    <th>Course Number</th>
    <th>Grade</th>
  </tr>
  <%  @studentGrades.each_with_index do |transcript, index| %>
  <tr>
    <td>CSE <%= transcript.course_id %></td>
    <td><%= @grades[index] %></td>
  </tr>
  <% end %>
</table>